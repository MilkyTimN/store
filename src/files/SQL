create table tb_category(
    id serial PRIMARY KEY,
    name varchar
);

create table tb_product(
    id serial PRIMARY KEY ,
    name varchar,
    categoryId int,
    Foreign Key (categoryId) REFERENCES tb_category(id)
);

create table tb_productDetails(
    id serial PRIMARY KEY,
    productId int,
    brand varchar,
    price float,
    description varchar,
    quantity int
);